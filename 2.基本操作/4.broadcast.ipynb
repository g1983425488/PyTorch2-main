    "c = torch.randn(1)\n",
    "\n",
    "print((a + b).shape)\n",
    "print((a + c).shape)\n",
    "\n",
    "#手动boradcast\n",
    "print(b.expand_as(a).shape)\n",
    "print(c.expand_as(a).shape)"
